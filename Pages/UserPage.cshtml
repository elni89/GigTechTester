@page
@model GigTechTester.Pages.UserPage
@{
}
<table>
    <thead>
        <tr>
            <th>First Name:</th>
            <th>Last Name:</th>
            <th>Email:</th>
            <th>Phone:</th>
            <th>Username</th>
            <th>Profile URL: </th>
            <th>Balance:th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model.Customers)
        {
            <tr>
                <td>@customer.first_name</td>
                <td>@customer.last_name</td>
                <td>@customer.email</td>
                <td>@customer.phone_number</td>
                <td>@customer.username</td>
                <td>@customer.profileImage</td>
                <td>@customer.vmoney</td>
            </tr>
        }
    </tbody>
</table>